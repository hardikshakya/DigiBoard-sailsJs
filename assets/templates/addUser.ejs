<tr data-id="<%- user.id %>" data-model="user">
	<% if (user.online) { %>
		<td><img src="./images/icon-online.png"></td>
	<% } else { %>
		<td> <img src="./images/icon-offline.png"></td>
	<% } %>
	<td><%- user.id %></td>
	<td><%- user.name %></td>
	<td><%- user.email %></td>
	<td>
		<% if (user.role === 'publisher') { %>
			<img src="/images/admin.png">publisher
		<% } if(user.role === 'advertiser') { %>
			<img src="/images/pawn.png">advertiser
		<% } %>
	</td>
	<td><a href="/user/showprofile/<%= user.id %>" class="btn btn-sm btn-primary">Show</a></td>
    <td><a href="/user/edituser/<%= user.id %>" class="btn btn-sm btn-warning">Edit</a></td>
	<td><form action="/user/destroyuser/<%- user.id %>" method="POST">
		<input type="hidden" name="_method" value="delete"/>
		<input type="submit" class="btn btn-sm btn-danger" value="Delete"/>
		<input type="hidden" class="_csrf" name="_csrf" value="<%- _csrf %>" />
	</form></td>
</tr>
